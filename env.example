# Environment Variables Configuration
# Copy this file to .env.local and add your actual API keys

# OpenWeatherMap API Key (Required)
# For Next.js, use NEXT_PUBLIC_ prefix
# For Create React App, use REACT_APP_ prefix
NEXT_PUBLIC_OPENWEATHER_API_KEY=your_openweather_api_key_here
# REACT_APP_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Google Pollen API Key (Required for real pollen data)
# For Next.js, use NEXT_PUBLIC_ prefix
# For Create React App, use REACT_APP_ prefix
# Get your API key from: https://developers.google.com/maps/documentation/pollen
NEXT_PUBLIC_GOOGLE_POLLEN_API_KEY=your_google_pollen_api_key_here
# REACT_APP_GOOGLE_POLLEN_API_KEY=your_google_pollen_api_key_here

# Development Environment
NODE_ENV=development

# Weather API Keys
OPENWEATHER_API_KEY=your_openweather_api_key_here
GOOGLE_AIR_QUALITY_API_KEY=your_google_air_quality_api_key_here

# Clerk Authentication (v0.2.9)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_YOUR_KEY_HERE
CLERK_SECRET_KEY=sk_test_YOUR_SECRET_HERE

# Clerk URLs
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/weather
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/onboarding

# Vercel Environment
NEXT_PUBLIC_VERCEL_ENV=preview 