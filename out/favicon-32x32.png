<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <!-- 32x32 16-bit pixelated weather icon with transparent background -->
  
  <!-- Sun rays (cyan) -->
  <rect x="15" y="2" width="2" height="4" fill="#00d4ff"/>
  <rect x="15" y="26" width="2" height="4" fill="#00d4ff"/>
  <rect x="2" y="15" width="4" height="2" fill="#00d4ff"/>
  <rect x="26" y="15" width="4" height="2" fill="#00d4ff"/>
  
  <!-- Diagonal rays -->
  <rect x="6" y="6" width="2" height="2" fill="#00d4ff"/>
  <rect x="24" y="6" width="2" height="2" fill="#00d4ff"/>
  <rect x="6" y="24" width="2" height="2" fill="#00d4ff"/>
  <rect x="24" y="24" width="2" height="2" fill="#00d4ff"/>
  
  <!-- Sun body (outer ring - yellow) -->
  <rect x="10" y="8" width="12" height="2" fill="#ffe66d"/>
  <rect x="8" y="10" width="2" height="12" fill="#ffe66d"/>
  <rect x="22" y="10" width="2" height="12" fill="#ffe66d"/>
  <rect x="10" y="22" width="12" height="2" fill="#ffe66d"/>
  
  <!-- Sun body corners -->
  <rect x="10" y="10" width="2" height="2" fill="#ffe66d"/>
  <rect x="20" y="10" width="2" height="2" fill="#ffe66d"/>
  <rect x="10" y="20" width="2" height="2" fill="#ffe66d"/>
  <rect x="20" y="20" width="2" height="2" fill="#ffe66d"/>
  
  <!-- Sun inner area (golden yellow) -->
  <rect x="12" y="10" width="8" height="2" fill="#ffcc02"/>
  <rect x="10" y="12" width="12" height="8" fill="#ffcc02"/>
  <rect x="12" y="20" width="8" height="2" fill="#ffcc02"/>
  
  <!-- Sun center highlight (white) -->
  <rect x="14" y="14" width="4" height="4" fill="#ffffff"/>
</svg> 