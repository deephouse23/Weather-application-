<svg width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
  <!-- 16x16 16-bit pixelated weather icon with transparent background -->
  
  <!-- Sun rays (simplified for 16x16 - cyan) -->
  <rect x="7" y="1" width="2" height="2" fill="#00d4ff"/>
  <rect x="7" y="13" width="2" height="2" fill="#00d4ff"/>
  <rect x="1" y="7" width="2" height="2" fill="#00d4ff"/>
  <rect x="13" y="7" width="2" height="2" fill="#00d4ff"/>
  
  <!-- Sun body (outer - yellow) -->
  <rect x="5" y="4" width="6" height="1" fill="#ffe66d"/>
  <rect x="4" y="5" width="1" height="6" fill="#ffe66d"/>
  <rect x="11" y="5" width="1" height="6" fill="#ffe66d"/>
  <rect x="5" y="11" width="6" height="1" fill="#ffe66d"/>
  
  <!-- Sun body corners -->
  <rect x="5" y="5" width="1" height="1" fill="#ffe66d"/>
  <rect x="10" y="5" width="1" height="1" fill="#ffe66d"/>
  <rect x="5" y="10" width="1" height="1" fill="#ffe66d"/>
  <rect x="10" y="10" width="1" height="1" fill="#ffe66d"/>
  
  <!-- Sun inner (golden yellow) -->
  <rect x="6" y="5" width="4" height="1" fill="#ffcc02"/>
  <rect x="5" y="6" width="6" height="4" fill="#ffcc02"/>
  <rect x="6" y="10" width="4" height="1" fill="#ffcc02"/>
  
  <!-- Sun center (white) -->
  <rect x="7" y="7" width="2" height="2" fill="#ffffff"/>
</svg> 